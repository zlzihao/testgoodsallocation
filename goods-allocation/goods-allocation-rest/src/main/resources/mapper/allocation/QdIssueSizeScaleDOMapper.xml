<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.nome.saas.allocation.repository.dao.allocation.QdIssueSizeScaleDOMapper" >

    <resultMap id="BaseResultMap" type="cn.nome.saas.allocation.repository.entity.allocation.QdIssueSizeScaleDO" >
        <result column="area_name" property="areaName" />
        <result column="category_name" property="categoryName" />
        <result column="mid_category_name" property="midCategoryName" />
        <result column="model_type" property="modelType" />
        <result column="size_name" property="sizeName" />
        <result column="percentage" property="percentage" />
    </resultMap>

    <select id = "getSizeScaleList" resultMap="BaseResultMap">
        select area_name,category_name,mid_category_name,model_type,size_name,percentage
        from qd_issue_size_scale
    </select>

</mapper>