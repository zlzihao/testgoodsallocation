<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.nome.saas.allocation.repository.dao.allocation.QdIssueShopListDOMapper" >

    <resultMap id="BaseResultMap" type="cn.nome.saas.allocation.repository.entity.allocation.QdIssueShopListDO" >
        <result column="area_name" property="areaName" />
        <result column="region_name" property="regionName" />
        <result column="province_name" property="provinceName" />
        <result column="shop_code" property="shopCode" />
        <result column="shop_id" property="shopId" />
        <result column="shop_name" property="shopName" />
        <result column="business_level" property="businessLevel" />
        <result column="area_level" property="areaLevel" />
        <result column="skc_width_suggest" property="skcWidthSuggest" />
        <result column="old_skc_percentage_suggest" property="oldSkcPercentageSuggest" />
    </resultMap>

    <select id = "getShopList" resultMap="BaseResultMap">
        select area_name,region_name,province_name,shop_code,shop_id,shop_name,business_level,area_level,skc_width_suggest,old_skc_percentage_suggest
        from qd_issue_shop_list
        order by created_at desc
    </select>



</mapper>